$("#star_" + <%= @video.id %>).raty({
  readOnly: true,
  score: <%= @video.average_rating %>,
  path: '/'
});

$("#user_star_" + <%= @video.id %>).raty({
  score: <%= @rating.score %>,
  path: '/',
  click: function(score, evt) {
    $.ajax({
      url: '/ratings/' + <%= @rating.id %>,
      type: 'PATCH',
      data: { score: score }
    }).done(function (data){});
  }
});