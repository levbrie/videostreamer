


<div class="col-lg-12">
  <!-- .crousel fade -->
  <section class="panel bg-dark">
    <div class="carousel slide carousel-fade panel-body" id="c-fade">
        <ol class="carousel-indicators out">
          <li data-target="#c-fade" data-slide-to="0" class="active"></li>
          <li data-target="#c-fade" data-slide-to="1" class=""></li>
        </ol>
        <div class="carousel-inner">
          <div class="item active">
            <p class="text-center">
              <em class="h4 text-mute">Upload a Video Now!</em><br>
              <%= link_to "Upload Video", new_video_path, class: "btn btn-sm btn-info" %>
            </p>
          </div>
          <div class="item">
            <p class="text-center">
              <em class="h4 text-mute">And Rate them right then and there!</em><br>
              <small class="text-muted">Only one rating per person</small>
            </p>
          </div>
        </div>
        <a class="left carousel-control" href="#c-fade" data-slide="prev">
          <i class="icon-angle-left"></i>
        </a>
        <a class="right carousel-control" href="#c-fade" data-slide="next">
          <i class="icon-angle-right"></i>
        </a>
    </div>
  </section>
  <!-- / .carousel fade -->
</div>
